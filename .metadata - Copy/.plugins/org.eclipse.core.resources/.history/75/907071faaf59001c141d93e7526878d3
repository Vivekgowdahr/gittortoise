package com.te.assignment;

public class IsPrime2 {
public static boolean isprime(int n) {
	for (int i = 1; i < n; i++) {
		if (n%i==0) {
			return false;
		}
	}return true;
}
public static void main(String[] args) {
	int num1=2;
	int num2=50;
	for (int i = num1; i < num2; i++) {
		if (isprime(i)) {
			System.out.println(i);
		}
	}
}
}
