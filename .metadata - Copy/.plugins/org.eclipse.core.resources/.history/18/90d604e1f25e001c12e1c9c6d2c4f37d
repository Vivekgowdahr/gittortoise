package com.te.customeexce;

public class ExceptionClass {
public void wrongaccess ()throws YouAreNotEligable {
	System.out.println("Hello... sir");
	throw new YouAreNotEligable("Sorry sir u are not eligibal");
}
public static void main(String[] args) {
	ExceptionClass exceptionClass = new ExceptionClass();
	try {
		exceptionClass.
	} catch (YouAreNotEligable e) {
		System.out.println(e.message);
	}
	}
}
